{
    "name": "farums/psr-container-doctrine",
    "type": "library",
    "version": "1.1.0",
    "description": "Doctrine Factories for PSR-11 Containers",
    "homepage": "https://github.com/farums/psr-container-doctrine",
    "license": "BSD-2-Clause",
    "require": {
        "php": ">=7.4.0,<7.5.0",
        "doctrine/common": "^2.13",
        "doctrine/dbal": "^2.10",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.7",
        "psr/container": "^1.0.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PsrContainerDoctrine\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}